#! /usr/bin/perl -w

use strict;

use Math::BigInt ':constant';

my @seq;

while (my $value = <>) {
  $seq[0]=Math::BigInt->new($value);
  if (defined $seq[3]) {
    my $x = $seq[2] - $seq[3];
    my $y = $seq[1] - $seq[2];
    my $z = $seq[0] - $seq[1];
    print "$x $y $z\n";
  }
  if (defined $seq[2]) {
    $seq[3] = $seq[2];
  }
  if (defined $seq[1]) {
    $seq[2] = $seq[1];
  }
  if (defined $seq[1]) {
    $seq[2] = $seq[1];
  }
  $seq[1] = $seq[0];
}
