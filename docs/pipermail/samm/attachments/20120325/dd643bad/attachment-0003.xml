<?xml version="1.0" encoding="iso-8859-1"?>
<maturity>
	<title>Example Organization Scorecard</title>
	<!-- The title that will be displayed below the scorecard -->
	<scorecard>
		<label>Actual Status at April 2012</label>
		<!-- The label is displayed at the top of the scorecard -->
		<levels>
			<!-- The maturity level for each practice; all security practices that should appear on the scorecard -->
			<level security-practice="SM" value="2"/>
			<level security-practice="PC" value="1"/>
			<level security-practice="EG" value="2"/>
			<level security-practice="TA" value="2"/>
			<level security-practice="SR" value="2+"/>
			<level security-practice="SA" value="0"/>
			<level security-practice="DR" value="1"/>
			<level security-practice="CR" value="1"/>
			<level security-practice="ST" value="2"/>
			<level security-practice="VM" value="0+"/>
			<level security-practice="EH" value="1"/>
			<level security-practice="OE" value="0"/>
		</levels>
	</scorecard>
</maturity>